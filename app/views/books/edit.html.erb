<body>
  
<%= form_with model: @book do |f| %>
  
<p>
  <strong>Title:</strong>
   <%= f.text_field :title %>
 
</p>

<p>
  <strong>Body:</strong>
  <%= f.text_area :body %>
  
</p>

<%= f.submit '投稿' %>

<% end %>
  
<%= link_to "Edit", edit_book_path(@book.id)%>
<%= link_to "Back", books_path(@book.id)%>
  
  <p id="notice"></p>
</body>